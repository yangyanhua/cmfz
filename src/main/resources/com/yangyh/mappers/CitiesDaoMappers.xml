<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.yangyh.dao.CitiesDao">
  <!--查询省  -->
  <select id="findprovince" resultType="cities">
  select * from cities where parentid is null order by id
  </select>
  
  <!--查询市  -->
  <select id="findcity" resultType="cities">
  select * from cities where parentid=#{parentid} order by id
  </select>
</mapper>

